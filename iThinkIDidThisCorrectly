public class AQuoteToBeRemembered
{
    public static void main (String[] args)
    {
        System.out.println (" Matt Groening once said: \"Love is a snowmobile racing across the tundra \n and then suddenly "
         + "it flips over, pinning you underneath\"");
        
        
    }//End of public static void main
    
} //End of public class
/**
 * Sidney Robinson
 * 1.5 
 * 8/17/17
 **/
 public class AllAboutMe
 {
     public static void main(String[] args)
     {
         System.out.println ("Name:\t Sidney \"Coolbust\" Robinson");
         
         System.out.println ("Date of Birth:\t 01\\16\\2000");
         
         System.out.println ("Hobbies:\t Snowboarding,Meme Collecting");
         
         System.out.println ("Favorite Book:\t \'THe Hitchhiker's Guide TO The Galaxy\'");
         
         System.out.println ("Favorite Movie:\t \"Inception\"");
          
         
          
         
         
     }//End method main
     
 }// ENd of Public class AllABoutMe




