public class Initials
{
    public static void main(String args[])
    {
        System.out.println("SSSSSSSSSSSS"   +       "\t RRRRRRRRRRRRR");
        System.out.println("SSSSS"          +   "\t\t RRRRRRRRRRRRR");
        System.out.println("SSSSS"          +   "\t\t RRRRRR");
        System.out.println("SSSSS"          +   "\t\t RRRRRR");
        System.out.println("SSSSSSSSSSS"    +     "\t RRRRRR");
        System.out.println("      sssss"    +     "\t RRRRRR");
        System.out.println("      sssss"    +     "\t RRRRRR");   
        System.out.println("      sssss"    +     "\t RRRRRR");      
        System.out.println("      sssss"     +    "\t RRRRRR");       
        System.out.println("      sssss"    +     "\t RRRRRR");      
        System.out.println("sssssssssss"    +     "\t RRRRRR");      
        
        

        
     
    
        
    }//End of S.V main
    
} //End of public class Initials



public class APoemForMe
{
    public static void main (String args[])
    {
        System.out.println ("Here you go" +
"\nlight low and long" +
"\nin the fields" +
"\nat sunset and sunrise" +
"\nEverything twice" +
"\na doubled existence" +
"\ntwo nows" +
"\ntwo thens" +
"\ntwo names" +
"\nyours and the other one" +
"\nalso yours" +
"\nfolded into a paper boat" +
"\nthe points of which" +
"\nconstellate stars" );
        
    }//End of public static void main
    
}//End of public class APoemFOrME




