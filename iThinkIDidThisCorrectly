 public class AllAboutMe
 {
     public static void main(String[] args)
     {
         System.out.println ("Name: Sidney Robinson");
         
         System.out.println ("Date of Birth: 1/16/2000");
         
         System.out.println ("Hobbies: Snowboarding,Meme Collecting");
         
         System.out.println ("Favorite Book: THe Hitchhiker's Guide TO The Galaxy");
         
         System.out.println ("Favorite Movie: Inception");
          
         
          
         


         
     }//End method main
     
 }// ENd of Public class AllABoutMe



